package practice;

public class practice {

	public static void main(String[] args) {
//		int[] A= {5, 10, 20, 100, 105};
		int[] A= {1, 2, 3, 4, 5};
//		int[] ans= solve(A, 110);
		int[] ans= solve(A, 5);
		for (int i = 0; i < ans.length; i++) {
			System.out.println(ans[i]+" ");
		}
	}
	public static int[] solve(int[] A, int B) {
        int start=0, end=0, flag=0, n=A.length, sum=0;
        
        for(int i=0; i<n; i++){
        	 flag=0;
            sum+=A[i];
            if(A[i]==B){
                start=i;end=i; flag=1; break;
            }
            else if(sum == B){
                end=i; flag=1; break;
            }
            else if(sum>B){
                if(A[i]<B){
                    for(int j=start; j<i; j++){
                        sum-=A[start];
                        start++;

                        if(sum==B){
                            end=i; flag=1; break;
                        }
                        else if(sum<B){
                            break;
                        }
                    }
                    if(flag==1) break;
                }
                else{
                    start=i+1;
                }
            }
           
        }
        if(flag==1){
            int[] ans= new int[end-start+1];
            for(int i=start; i<=end; i++){
                ans[i-start]=A[i];
            }
            return ans;
        }
        else{
            int[] ans={-1};
            return ans;
        }
    }
}
